import statistics as stat 
num1 = int(input ())
num2 = int(input ())
num3 = int(input ())
sp = [num1, num2, num3]
print('{0:.6f}'.format(stat.mean(sp)), end=" ; ")
print('{0:.6f}'.format(stat.median(sp)), end=" ; ")
print('{0:.6f}'.format(stat.harmonic_mean(sp)), end=" ; ")
print(max(sp) ** min(sp) / (stat.mean(sp)))
